{
  "name": "apitte/negotiation",
  "description": "Content-Negotiation support for Apitte stack",
  "keywords": [
    "apitte",
    "negotiation",
    "content-negotiation",
    "view-resolver"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/negotiation",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "apitte/core": "^0.7.0"
  },
  "require-dev": {
    "nette/di": "~3.0.0",
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Negotiation\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
