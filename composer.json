{
  "name": "apitte/negotiation",
  "description": "Content-Negotiation support for Apitte stack",
  "keywords": [
    "apitte",
    "negotiation",
    "content-negotiation",
    "view-resolver"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/negotiation",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.3",
    "apitte/core": "~0.7 || ~0.8"
  },
  "require-dev": {
    "nette/di": "^3.0.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Negotiation\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
